/* FAQ */
"faq_language_switch_question" = "मैं ऐप की भाषा कैसे बदलूं?";
"faq_language_switch_answer" = "ऐप की भाषा बदलने के लिए: 1) सेटिंग्स पर जाएं, 2) 'भाषा चयन' पर टैप करें, 3) अपनी पसंदीदा भाषा चुनें, 4) परिवर्तनों को प्रभावी बनाने के लिए ऐप को बंद करें और फिर से खोलें। ऐप भविष्य के सत्रों के लिए आपकी भाषा पसंद को याद रखेगा।";
"faq_step_counting_question" = "कदम गिनती कैसे काम करती है?";
"faq_step_counting_answer" = "मोविक स्टेप्स आपके डिवाइस के अंतर्निहित एक्सेलेरोमीटर और जाइरोस्कोप का उपयोग करके आपकी चलने की गति का पता लगाता है। ऐप लगातार आपके गति पैटर्न की निगरानी करता है और दिन भर में आपके द्वारा उठाए गए हर कदम को गिनता है।";
"faq_healthkit_sync_question" = "मेरे कदम HealthKit के साथ क्यों सिंक नहीं हो रहे?";
"faq_healthkit_sync_answer" = "सुनिश्चित करें कि आपने मोविक स्टेप्स को HealthKit में डेटा पढ़ने और लिखने की अनुमति दी है। सेटिंग्स > गोपनीयता और सुरक्षा > स्वास्थ्य > मोविक स्टेप्स पर जाएं और सभी अनुमतियों को सक्षम करें। अनुमतियां देने के बाद आपको ऐप को पुनः आरंभ करना पड़ सकता है।";
"faq_accuracy_question" = "कदम गिनती कितनी सटीक है?";
"faq_accuracy_answer" = "मोविक स्टेप्स तब बहुत सटीक होता है जब आपका फोन आपकी जेब या बैग में होता है। सटीकता इस बात पर निर्भर कर सकती है कि आप अपना फोन कैसे रखते हैं और आपका चलने का पैटर्न कैसा है। सबसे अच्छे परिणामों के लिए, चलते समय अपना फोन जेब में रखें।";

